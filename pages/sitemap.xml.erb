---
layout: false
---

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <% pages = Sitepress::Site.new.resources.glob("*html*") %>
  <% pages.each do |page| %>
  <url>
    <loc>https://industrially.net<%= page.request_path %></loc>
    <% if page.data.date %>
      <lastmod><%= page.data.date.strftime("%Y-%m-%d") %></lastmod>
    <% end %>
  </url>
  <% if page.children.any? %>
  <% page.children.each do |child| %>
    <url>
      <loc>https://industrially.net<%= child.request_path %></loc>
      <% if child.data.date %>
        <lastmod><%= child.data.date.strftime("%Y-%m-%d") %></lastmod>
      <% end %>
    </url>
  <% end %>
  <% end %>
  <% end %>
</urlset>